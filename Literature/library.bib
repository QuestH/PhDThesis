   issn = {23519789},
   journal = {Procedia Manufacturing},
   keywords = {Combined bearings,Control,Stiffness},
   pages = {650-657},
   publisher = {Elsevier B.V.},
   title = {Operational effects on the stiffness of combined roller and plain bearings},
   volume = {41},
