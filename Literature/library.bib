@article{Meged2017,
   abstract = {The service life of a single rolling contact bearing (RCB) cannot be predicted, due to the large scatter in bearing life. However, the service life of a group of identical bearings tested under identical conditions follows a typical pattern. Many theories that were established to describe this pattern are based on failure caused by fatigue due to cyclic loading. This loading causes high stresses and subsurface cracks. The cracks propagate and, once they reach the surface, cause bearing failure. These theories focus on the rolling elements and disregard the lubricant. This study is concerned with the failure mechanism caused by cavitation of the lubricant and presents its significant role in RCB failure. Both mechanisms act simultaneously and impose synergistic effects. It is therefore recommended to combine these theories into a unified concept.},
   author = {Y. Meged},
   doi = {10.1520/MPC20170023},
   issn = {23791365},
   issue = {1},
